<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sport_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_sport_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_sport_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ExerciseActivity"
            android:label="@string/title_activity_my_exercises"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SessionActivity"
            android:label="@string/title_activity_show_exercise"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".ListSessionActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".TrainingActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>